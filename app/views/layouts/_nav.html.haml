.navbar
  .navbar-inner
    %a.brand.pull-right{:href => "/"}
      .logo
        =image_tag "logo.png"
    %ul.nav.pull-right{:id => "nav-links"}
      %li
        %a{:href => "/#portfolio"} Get started
      %li
        %a{:href => "/static_pages/about"} What we do
      %li
        - if user_signed_in? 
          = link_to 'Sign out', destroy_user_session_path, :method => :delete
        - else
          = link_to 'Sign in', '/users/sign_in'